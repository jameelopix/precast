<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow.xsd">

 <input name="purchaseRegisterServiceRequest" required="true" />

 <action-state id="getPurchaseRegisters">
 <evaluate
 expression="purchaseRegisterComponent.getPurchaseRegisters(purchaseRegisterServiceRequest)"
 result="flowScope.purchaseRegisterServiceResponse" />
 <transition to="exit" />
 </action-state>

 <end-state id="exit">
 <output name="purchaseRegisterServiceResponse" />
 </end-state>
</flow>